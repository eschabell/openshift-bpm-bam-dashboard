parameter.currentPageTemplate1.default=<div style="border-top: 1px dotted gray; padding: 6px 10px; color: #33455b; font-size: 14px;"><b>{PAGE}</b></div>

parameter.PageTemplate2.default=<div style="border-top: 1px dotted gray; padding: 6px 0px 6px 25px; color: #33455b; font-size: 13px;"><a href="{LINK}">{PAGE}</a></div>
parameter.currentPageTemplate2.default=<div style="border-top: 1px dotted gray; padding: 6px 0px 6px 25px; color: #33455b; font-size: 13px;"><b>{PAGE}</b></div>

parameter.PageTemplate3.default=<div style="border-top: 1px dotted gray; padding: 6px 0px 6px 35px; color: #33455b; font-size: 12px;"><a href="{LINK}">&middot; {PAGE}</a></div>
parameter.currentPageTemplate3.default=<div style="border-top: 1px dotted gray; padding: 6px 0px 6px 35px; color: #33455b; font-size: 12px;"><b>&middot; {PAGE}</b></div>

