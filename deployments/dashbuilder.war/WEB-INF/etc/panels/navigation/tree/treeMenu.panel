panel.id          = navigation.tree_menu
panel.description = navigation.tree_menu.description
panel.group       = panel.group.navigation
panel.driver      = org.jboss.dashboard.ui.panel.navigation.treeMenu.TreeMenuDriver

panel.thumbnail =/panels/thumbnails/menu.png

jsp.show        = /panels/navigation/tree_menu/show.jsp
jsp.edit        = /panels/navigation/tree_menu/edit.jsp

resources.messages=./messages.properties


