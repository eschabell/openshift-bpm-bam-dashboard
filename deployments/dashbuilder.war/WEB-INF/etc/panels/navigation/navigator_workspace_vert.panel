#
# Language selector (horizontal)
#

panel.id          = navigation.workspace_vert
panel.description = navigation.workspace_vert.description
panel.group       = panel.group.navigation
panel.driver      = org.jboss.dashboard.ui.panel.navigation.menu.MenuDriver

panel.thumbnail =/panels/thumbnails/menu.png

# logical pages to JSPs mappings
jsp.show        = /panels/navigation/menu/show.jsp
jsp.edit.lang   = /panels/navigation/menu/edit_lang.jsp
jsp.edit.page   = /panels/navigation/menu/edit_page.jsp
jsp.edit.workspace = /panels/navigation/menu/edit_workspace.jsp

resources.messages=./messages.properties

parameter.menu.type             = workspace
parameter.menu.type.default     =

parameter.start.html            = <table border="0" cellpadding="0" cellspacing="0" align="center">
parameter.start.html.default    =
parameter.end.html              = </table>
parameter.end.html.default      =

parameter.before.link           = <tr><td valign="middle" align="center" nowrap>
parameter.before.link.default   =
parameter.after.link            = </td></tr>
parameter.after.link.default    =
